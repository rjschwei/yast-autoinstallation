<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns:config="http://www.suse.com/1.0/configns" ns="http://www.suse.com/1.0/yast2ns" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <include href="nfs.rng"/>
  <include href="nis.rng"/>
  <include href="sysconfig.rng"/>
  <include href="kerberos_client.rng"/>
  <define name="install">
    <element name="install">
		<empty/>
    </element>
  </define>
  <define name="att.config">
    <attribute name="config:type">
   		<choice>
      		<value>list</value>
      		<value>integer</value>
      		<value>symbol</value>
      		<value>boolean</value>
  		</choice>
     </attribute>
  </define>
  <define name="configure">
    <element name="configure">
      <optional>
        <ref name="kerberos_client"/>
      </optional>
      <optional>
        <ref name="nfs"/>
      </optional>
      <optional>
        <ref name="nis"/>
      </optional>
      <optional>
        <ref name="sysconfig"/>
      </optional>
    </element>
  </define>
  <define name="profile">
    <element name="profile">
      <optional>
        <ref name="configure"/>
      </optional>
      <optional>
        <ref name="install"/>
      </optional>
    </element>
  </define>
  <start>
      <ref name="profile"/>
  </start>
</grammar>
